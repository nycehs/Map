{
    "$schema": "https://vega.github.io/schema/vega-lite/v4.json",
    "width": 400,
    "height": 400,
    "data": {
      "url": "ZCTA.geo.json",
      "format": {"property": "features"}
    },
    "transform": [{
      "lookup": "properties.MODZCTA",
      "from": {
        "data": {
          "url": "FakeZCTAData.csv"
        },
        "key": "zcta",
        "fields": ["time","cases","rate","pctpos"]
      }
    }],
    "mark": "geoshape",
    "encoding": {
      "color": {
          "field":"cases","type":"quantitative"
      },
        
      "tooltip": [
        {"field": "zcta", "type": "nominal", "title":"ZipCode"},
        {"field": "cases", "type": "quantitative", "title":"Cases"},
        {"field": "rate", "type": "quantitative", "title":"Rate"},
        {"field": "pctpos", "type": "quantitative", "title":"Percent Positive"}
      ]
    }
  }